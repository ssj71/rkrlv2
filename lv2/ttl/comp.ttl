<http://rakarrack.sourceforge.net/effects.html#comp>
        a lv2:Plugin, lv2:CompressorPlugin ;
        doap:name "rkr Compressor" ;
        doap:description "compressor";
        lv2:project <http://rakarrack.sourceforge.net/effects.html>;
        lv2:minorVersion 0 ;
        lv2:microVersion 0 ;
        rdfs:comment "A flexible compressor with optional soft knee and makeup gain." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <http://rakarrack.sourceforge.net/effects.html#comp_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "THRESHOLD" ;
                lv2:name "Threshold" ;
                lv2:default -30 ;
                lv2:minimum -60 ;
                lv2:maximum -3 ;
                lv2:portProperty lv2:integer ;
                units:unit units:db;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "RATIO" ;
                lv2:name "RATIO" ;
                lv2:default 2 ;
                lv2:minimum 2 ;
                lv2:maximum 42 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Output Gain" ;
                lv2:default -6 ;
                lv2:minimum -40 ;
                lv2:maximum 0 ;
                lv2:portProperty lv2:integer ;
                units:unit units:db;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "ATTACK" ;
                lv2:name "Attack Time" ;
                lv2:default 20 ;
                lv2:minimum 10 ;
                lv2:maximum 250 ;
                lv2:portProperty lv2:integer ;
                units:unit units:ms;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "RELEASE" ;
                lv2:name "Release Time" ;
                lv2:default 120 ;
                lv2:minimum 10 ;
                lv2:maximum 500 ;
                lv2:portProperty lv2:integer ;
                units:unit units:ms;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "AUTOOUT" ;
                lv2:name "Auto Output" ;
                lv2:default 1 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "KNEE" ;
                lv2:name "Knee" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 100 ;
                lv2:portProperty lv2:integer ;
                units:unit units:pc;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "STEREO" ;
                lv2:name "Stereo" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "PEAK" ;
                lv2:name "Peak" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ] ;
	.

