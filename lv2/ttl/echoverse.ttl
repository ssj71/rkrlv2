<http://rakarrack.sourceforge.net/effects.html#Echoverse>
        a lv2:Plugin, lv2:ReverbPlugin ;
        doap:name "rkr Echoverse" ;
        doap:description "Thick Echo";
        lv2:project <http://rakarrack.sourceforge.net/effects.html>;
        lv2:minorVersion 0 ;
        lv2:microVersion 0 ;
        rdfs:comment "Configurable echo designed to be stable for thick, in-your-face walls of sound." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <http://rakarrack.sourceforge.net/effects.html#Echoverse_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "WETDRY" ;
                lv2:name "Wet/Dry" ;
                lv2:default 67 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "PAN" ;
                lv2:name "Panning" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "DELAY" ;
                lv2:name "TEMPO" ;
                lv2:default 128 ;
                lv2:minimum 1 ;
                lv2:maximum 600 ;
                lv2:portProperty lv2:integer ;
                units:unit units:bpm ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "LR_DELAY" ;
                lv2:name "Left/Right Delay Offset" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "ANGLE" ;
                lv2:name "Angle" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "FEEDBACK" ;
                lv2:name "Feedback" ;
                lv2:default 59 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "DAMP" ;
                lv2:name "Damping" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "REV" ;
                lv2:name "Reverse" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "SUBD" ;
                lv2:name "Subdivision" ;
                lv2:default 1 ;
                lv2:minimum 1 ;
                lv2:maximum 6 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:enumeration;
                lv2:scalePoint [ rdfs:label "1"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "1/2"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "1/3"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "1/4"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "1/5"; rdf:value 5 ] ;
                lv2:scalePoint [ rdfs:label "1/6"; rdf:value 6 ] ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "ES" ;
                lv2:name "Extra Stereo" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ] ;
	.

<http://rakarrack.sourceforge.net/effects.html#Echoverse:preset:echo_1>
        a pset:Preset ;
        lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#Echoverse> ;
        rdfs:label "Echo 1" ;
        rdfs:seeAlso <echoverse_presets.ttl> .

<http://rakarrack.sourceforge.net/effects.html#Echoverse:preset:echo_2>
        a pset:Preset ;
        lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#Echoverse> ;
        rdfs:label "Echo 2" ;
        rdfs:seeAlso <echoverse_presets.ttl> .

<http://rakarrack.sourceforge.net/effects.html#Echoverse:preset:echo_3>
        a pset:Preset ;
        lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#Echoverse> ;
        rdfs:label "Echo 3" ;
        rdfs:seeAlso <echoverse_presets.ttl> .
