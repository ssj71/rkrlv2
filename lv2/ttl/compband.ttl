<http://rakarrack.sourceforge.net/effects.html#CompBand>
        a lv2:Plugin, lv2:CompressorPlugin ;
        doap:name "rkr CompBand" ;
        doap:description "Multi-Band Compressor";
        lv2:project <http://rakarrack.sourceforge.net/effects.html>;
        lv2:minorVersion 0 ;
        lv2:microVersion 0 ;
        rdfs:comment "Multi-band compressor with 4 frequency bands and wet/dry mix control. Each band has individual ratio and threshold controls." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <http://rakarrack.sourceforge.net/effects.html#CompBand_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "WETDRY" ;
                lv2:name "Wet/Dry" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "LR" ;
                lv2:name "Low Band Ratio" ;
                lv2:default 4 ;
                lv2:minimum 2 ;
                lv2:maximum 42 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "M1R" ;
                lv2:name "Mid Band 1 Ratio" ;
                lv2:default 4 ;
                lv2:minimum 2 ;
                lv2:maximum 42 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "M2R" ;
                lv2:name "Mid Band 2 Ratio" ;
                lv2:default 4 ;
                lv2:minimum 2 ;
                lv2:maximum 42 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "HR" ;
                lv2:name "High Band Ratio" ;
                lv2:default 4 ;
                lv2:minimum 2 ;
                lv2:maximum 42 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "LT" ;
                lv2:name "Low Band Threshold" ;
                lv2:default 0 ;
                lv2:minimum -70 ;
                lv2:maximum 24 ;
                lv2:portProperty lv2:integer ;
                units:unit units:db;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "M1T" ;
                lv2:name "Mid Band 1 Threshold" ;
                lv2:default 0 ;
                lv2:minimum -70 ;
                lv2:maximum 24 ;
                lv2:portProperty lv2:integer ;
                units:unit units:db;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "M2T" ;
                lv2:name "Mid Band 2 Threshold" ;
                lv2:default 0 ;
                lv2:minimum -70 ;
                lv2:maximum 24 ;
                lv2:portProperty lv2:integer ;
                units:unit units:db;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "HT" ;
                lv2:name "High Threshold" ;
                lv2:default 0 ;
                lv2:minimum -70 ;
                lv2:maximum 24 ;
                lv2:portProperty lv2:integer ;
                units:unit units:db;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "LMCR" ;
                lv2:name "Low/Mid1 Crossover" ;
                lv2:default 200 ;
                lv2:minimum 20 ;
                lv2:maximum 1000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 15 ;
                lv2:symbol "MMCR" ;
                lv2:name "Mid1/Mid2 Crossover" ;
                lv2:default 2000 ;
                lv2:minimum 1000 ;
                lv2:maximum 8000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 16 ;
                lv2:symbol "MHCR" ;
                lv2:name "Mid2/High Crossover" ;
                lv2:default 15000 ;
                lv2:minimum 2000 ;
                lv2:maximum 26000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 17 ;
                lv2:symbol "GAIN" ;
                lv2:name "Gain" ;
                lv2:default 88 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ] ;
	.


<http://rakarrack.sourceforge.net/effects.html#CompBand:preset:good_start>
        a pset:Preset ;
        lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#CompBand> ;
        rdfs:seeAlso <compband_presets.ttl> .

<http://rakarrack.sourceforge.net/effects.html#CompBand:preset:loudness>
        a pset:Preset ;
        lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#CompBand> ;
        rdfs:seeAlso <compband_presets.ttl> .

<http://rakarrack.sourceforge.net/effects.html#CompBand:preset:loudness2>
        a pset:Preset ;
        lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#CompBand> ;
        rdfs:seeAlso <compband_presets.ttl> .
