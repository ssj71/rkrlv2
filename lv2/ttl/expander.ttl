<http://rakarrack.sourceforge.net/effects.html#Expander>
        a lv2:Plugin, lv2:ExpanderPlugin ;
        doap:name "rkr Expander" ;
        doap:description "Dynamic Expander";
        lv2:project <http://rakarrack.sourceforge.net/effects.html>;
        lv2:minorVersion 0 ;
        lv2:microVersion 0 ;
        rdfs:comment "Analog BJT modeled dynamic expander for subtler noise suppression than a gate typically offers. Can also be used for interesting evelope-triggered swell and fade effects." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <http://rakarrack.sourceforge.net/effects.html#Expander_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "THRESHOLD" ;
                lv2:name "Threshold" ;
                lv2:default -30 ;
                lv2:minimum -80 ;
                lv2:maximum 0 ;
                lv2:portProperty lv2:integer ;
                units:unit units:db;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "SHAPE" ;
                lv2:name "Shape" ;
                lv2:default 2 ;
                lv2:minimum 1 ;
                lv2:maximum 50 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "ATTACK" ;
                lv2:name "Attack Time" ;
                lv2:default 20 ;
                lv2:minimum 1 ;
                lv2:maximum 5000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:ms;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "RELEASE" ;
                lv2:name "Release Time" ;
                lv2:default 120 ;
                lv2:minimum 10 ;
                lv2:maximum 1000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:ms;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "LPF" ;
                lv2:name "Lowpass Filter" ;
                lv2:default 3134 ;
                lv2:minimum 20 ;
                lv2:maximum 26000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "HPF" ;
                lv2:name "Highpass Filter" ;
                lv2:default 76 ;
                lv2:minimum 20 ;
                lv2:maximum 20000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Output Gain" ;
                lv2:default 10 ;
                lv2:minimum 1 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ] ;
	.

<http://rakarrack.sourceforge.net/effects.html#Expander:preset:noise_gate>
	a pset:Preset ;
	rdfs:seeAlso <expander_presets.ttl> ;
	lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#Expander> .

<http://rakarrack.sourceforge.net/effects.html#Expander:preset:boost_gate>
	a pset:Preset ;
	rdfs:seeAlso <expander_presets.ttl> ;
	lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#Expander> .

<http://rakarrack.sourceforge.net/effects.html#Expander:preset:treble_swell>
	a pset:Preset ;
	rdfs:seeAlso <expander_presets.ttl> ;
	lv2:appliesTo <http://rakarrack.sourceforge.net/effects.html#Expander> .
